gete:{
  e:alloc x; e[0]:2; p:x+10; a:1+0*til p
  N[x-1;
    carry:0; j:p-1
    W[j>0;
      temp:a[j]*10+carry
      a[j]:temp%j
      carry:floor(temp/j)
      j:j-1]
    e[_n+1]:carry%10]
  e}
echo gete 100

primes:{
  p: til[x + 1] > 1;limit: floor sqrt x;i: 2
  W[i <= limit;
    $.[p[i];
      j: i * i
      W[j <= x;
        p[j]: false
        j: j + i]]
    i: $[i = 2; 3; i + 2]]
  where[p]}
echo primes 100

