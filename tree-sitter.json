{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "wq",
      "camelcase": "Wq",
      "title": "wq",
      "scope": "source.wq",
      "file-types": ["wq"],
      "highlights": ["queries/highlights.scm"],
      "injection-regex": "^wq$",
      "class-name": "TreeSitterWq"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter for wq",
    "authors": [
      {
        "name": "tttiw"
      }
    ],
    "links": {
      "repository": "https://github.com/illusory-dept/tree-sitter-wq"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
